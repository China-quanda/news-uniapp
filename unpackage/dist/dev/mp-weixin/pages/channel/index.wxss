.shake-bottom.data-v-09f7f959 {
  animation: shake-bottom-09f7f959 0.8s cubic-bezier(0.255, 0.03, 0.215, 0.255) infinite both;
}
@keyframes shake-bottom-09f7f959 {
0%, 100% {
    transform: rotate(0deg);
    transform-origin: 50% 100%;
}
10% {
    transform: rotate(2deg);
}
20%, 40%, 60% {
    transform: rotate(-2deg);
}
30%, 50%, 70% {
    transform: rotate(2deg);
}
80% {
    transform: rotate(-2deg);
}
90% {
    transform: rotate(2deg);
}
}
